<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.milkteaboy.simplefarm.dao.BuildLevelinfoDao">

    <select id="selectByBuildIdAndLevel" resultType="org.milkteaboy.simplefarm.entity.BuildLevelinfo">
        select * from build_levelinfo
        where id=#{buildId} and level=#{level}
    </select>

</mapper>