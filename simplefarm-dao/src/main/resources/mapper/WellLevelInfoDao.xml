<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.milkteaboy.simplefarm.dao.WellLevelinfoDao">

    <select id="selectByLevel" parameterType="java.lang.Integer" resultType="org.milkteaboy.simplefarm.entity.WellLevelinfo">
        select * from well_levelinfo
        where level=#{value}
    </select>

</mapper>