<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.milkteaboy.simplefarm.dao.GoodsDao">
    
    <select id="selectById" parameterType="java.lang.Integer" resultType="org.milkteaboy.simplefarm.entity.Goods">
        select * from goods
        where id=#{value}
    </select>
    
    <select id="selectAll" resultType="org.milkteaboy.simplefarm.entity.Goods">
        select * from goods
    </select>
    
</mapper>